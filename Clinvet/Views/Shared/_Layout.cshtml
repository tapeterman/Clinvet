<!DOCTYPE html>
<html class="no-js" lang="pt-br">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport" />
    <meta name="description" content="Sistema de Controle para PetShop" />
    <title>SysComPet</title>
    @Styles.Render("~/assets/css")
</head>
<body class="theme-purple">
    <nav class="navbar p-l-5 p-r-5">
        <ul class="nav navbar-nav navbar-left">
            <li>
                <div class="navbar-header">
                    <a href="javascript:void(0);" class="bars"></a>
                    <a class="navbar-brand" href="">
                        <img src="~/assets/images/logo.svg" width="30" /><span class="m-l-10">SysComPet</span>
                    </a>
                </div>
            </li>
            <li><a href="javascript:void(0);" class="ls-toggle-btn" data-close="true"><i class="zmdi zmdi-swap"></i></a></li>
            <li class="hidden-sm-down">
                <div class="input-group">
                    <input type="text" class="form-control" placeholder="Search..." />
                    <span class="input-group-addon">
                        <i class="zmdi zmdi-search"></i>
                    </span>
                </div>
            </li>
            <li class="float-right">
                <a href="javascript:void(0);" class="fullscreen hidden-sm-down" data-provide="fullscreen" data-close="true"><i class="zmdi zmdi-fullscreen"></i></a>
                <a href="" class="mega-menu" data-close="true"><i class="zmdi zmdi-power"></i></a>
            </li>
        </ul>
    </nav>
    <aside id="leftsidebar" class="sidebar">
        <div class="tab-content">
            <div class="tab-pane stretchRight active" id="dashboard">
                <div class="menu">
                    <ul class="list">
                        <li>
                            <div class="user-info bg-purple">
                                <div class="image">
                                    <a href="">
                                        <img src="~/assets/images/lg/avatar1.jpg" alt="User" />
                                    </a>
                                </div>
                                <div class="detail">
                                    <small>Médico</small>
                                </div>
                            </div>
                        </li>
                        <li>
                            <a class="menu-toggle">
                                <i class="zmdi zmdi-home"></i>
                                <span>Home</span>
                            </a>
                            <ul class="ml-menu">
                                <li>
                                    <a href="@Url.Action("Index","Home")" class=" waves-effect waves-block ">
                                        <i class="zmdi zmdi-home"></i>
                                        <span>Início</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("About", "Home")" class=" waves-effect waves-block">
                                        <i class="zmdi zmdi-store"></i>
                                        <span>Sobre a Clinvet</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Contact", "Home")" class=" waves-effect waves-block">
                                        <i class="zmdi zmdi-hearing"></i>
                                        <span>Contatos</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a class="menu-toggle">
                                <i class="zmdi zmdi-accounts"></i>
                                <span>Funcionários</span>
                            </a>
                            <ul class="ml-menu">
                                <li class="header">Logins</li>
                                <li><a href="@Url.Action("Index", "Usuario")">Lista de Logins</a></li>
                                <li><a href="@Url.Action("Create", "Usuario")">Novo Login</a></li>
                                <li class="header">Funcionário</li>
                                <li><a href="@Url.Action("Index", "Funcionario")">Lista de Funcionários</a></li>
                                <li><a href="@Url.Action("Create", "Funcionario")">Novo Funcionário</a></li>
                            </ul>
                        </li>

                        <li>
                            <a class="menu-toggle">
                                <i class="material-icons">contacts</i>
                                <span>Produtos e Fornecedores</span>
                            </a>
                            <ul class="ml-menu">
                                <li>
                                    <a href="@Url.Action("Index", "ContasAPagar")" class=" waves-effect waves-block">
                                        <i class="material-icons">local_atm</i>
                                        <span>Contas a Pagar</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "Fornecedor")" class=" waves-effect waves-block">
                                        <i class="material-icons">local_shipping</i>
                                        <span>Fornecedores</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "ProdutoServico")" class=" waves-effect waves-block">
                                        <i class="material-icons">local_grocery_store</i>
                                        <span>Produtos e Serviços</span>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        <li>
                            <a class="menu-toggle">
                                <i class="material-icons">favorite</i>
                                <span>Clientes e Serviços</span>
                            </a>
                            <ul class="ml-menu">
                                <li>
                                    <a href="@Url.Action("Index", "Cliente")" class=" waves-effect waves-block">
                                        <i class="zmdi zmdi-thumb-up"></i>
                                        <span>Clientes</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "AgendamentoConsulta")" class=" waves-effect waves-block">
                                        <i class="material-icons">date_range</i>
                                        <span>Agendamentos</span>
                                    </a>
                                </li>

                                <li>
                                    <a href="@Url.Action("Index", "VendaServico")" class=" waves-effect waves-block">
                                        <i class="material-icons">local_grocery_store</i>
                                        <span>Venda</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li>
                            <a class="menu-toggle">
                                <i class="material-icons">loyalty</i>
                                <span>Meus Pets</span>
                            </a>
                            <ul class="ml-menu">
                                <li>
                                    <a href="@Url.Action("Index", "Animal")" class=" waves-effect waves-block">
                                        <i class="material-icons">pets</i>
                                        <span>Meus Pets</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "FichaAnimal")" class=" waves-effect waves-block">
                                        <i class="material-icons">local_offer</i>
                                        <span>Ficha Aimal</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "Vacina")" class=" waves-effect waves-block">
                                        <i class="material-icons">healing</i>
                                        <span>Vacina</span>
                                    </a>
                                </li>

                                <li>
                                    <a href="@Url.Action("Index", "Anamnese")" class=" waves-effect waves-block">
                                        <i class="material-icons">markunread_mailbox</i>
                                        <span>Prontuário Médico</span>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        <li>
                            <a class="menu-toggle">
                                <i class="material-icons">event_note</i>
                                <span>Tipos</span>
                            </a>
                            <ul class="ml-menu">
                                <li>
                                    <a href="@Url.Action("Index", "TiposDeVacina")" class=" waves-effect waves-block">
                                        <i class="material-icons">add_to_photos</i>
                                        <span>Tipo de Vacina</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "Tipo")" class=" waves-effect waves-block">
                                        <i class="material-icons">assignment_late</i>
                                        <span>Tipos de Produtos</span>
                                    </a>
                                </li>

                                <li>
                                    <a href="@Url.Action("Index", "TipoDeConta")" class=" waves-effect waves-block">
                                        <i class="material-icons">card_membership</i>
                                        <span>Tipo de Conta</span>
                                    </a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Index", "FormaDePagamento")" class=" waves-effect waves-block">
                                        <i class="material-icons">assignment</i>
                                        <span>Forma de Pagamento</span>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </aside>

    @RenderBody()
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/assets/bundles/js")
    @RenderSection("scripts", required: false)
</body>
</html>
